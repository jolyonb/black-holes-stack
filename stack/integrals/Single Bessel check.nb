Cell["Compare Integrals", "Section",ExpressionUUID->"9d977029-1fac-4033-a1ef-90d1088695ec"],

Cell["\<\
Compare python C integrals to analytical C integrals (test case) or numerical \
integrals (real case).\
\>", "Text",ExpressionUUID->"88b71fd5-ff14-49d5-be2b-46d489f3ab51"],

Cell[BoxData[
 RowBox[{"If", "[", 
  RowBox[{"testps", ",", "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"Test", " ", "case"}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Cint", "=", 
     RowBox[{"Integrate", "[", 
      RowBox[{
       RowBox[{"4", " ", "\[Pi]", " ", 
        RowBox[{"k", "^", "2"}], " ", 
        RowBox[{"Exp", "[", 
         RowBox[{"-", "k"}], "]"}], 
        RowBox[{"SphericalBesselJ", "[", 
         RowBox[{"0", ",", 
          RowBox[{"k", " ", "r"}]}], "]"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"k", ",", "kmin", ",", "kmax"}], "}"}]}], "]"}]}], ";", "\n", 
    
    RowBox[{"Cresults", "=", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"N", "[", 
        RowBox[{
         RowBox[{"Cint", "/.", 
          RowBox[{"r", "\[Rule]", "x"}]}], ",", "20"}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"x", ",", "rvals"}], "}"}]}], "]"}]}], ";"}], ",", 
   "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"Real", " ", "case"}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Cresults", "=", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{"NIntegrate", "[", 
        RowBox[{
         RowBox[{"4", "\[Pi]", " ", 
          RowBox[{"k", "^", "2"}], " ", 
          RowBox[{"Pk", "[", "k", "]"}], 
          RowBox[{"SphericalBesselJ", "[", 
           RowBox[{"0", ",", 
            RowBox[{"k", " ", "r"}]}], "]"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"k", ",", "kmin", ",", "kmax"}], "}"}], ",", " ", 
         RowBox[{"WorkingPrecision", "\[Rule]", "30"}]}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"r", ",", "rvals"}], "}"}]}], "]"}]}], ";"}]}], 
  "\[IndentingNewLine]", "]"}]], "Input",
 CellLabel->"In[47]:=",ExpressionUUID->"830f3792-a47f-4314-8454-76fff235113a"],

Cell[BoxData[{
 RowBox[{"ListLogLogPlot", "[", 
  RowBox[{
   RowBox[{"Transpose", "[", 
    RowBox[{"{", 
     RowBox[{"rvals", ",", " ", 
      RowBox[{"Abs", "[", 
       RowBox[{"Cresults", "-", "Cvals"}], "]"}]}], "}"}], "]"}], ",", 
   RowBox[{"PlotLabel", "\[Rule]", "\"\<Absolute difference\>\""}]}], 
  "]"}], "\[IndentingNewLine]", 
 RowBox[{"ListLogLogPlot", "[", 
  RowBox[{
   RowBox[{"Transpose", "[", 
    RowBox[{"{", 
