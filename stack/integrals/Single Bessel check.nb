  ";"}], "\[IndentingNewLine]", 
 RowBox[{"If", "[", 
  RowBox[{"testps", ",", "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"Test", " ", "power", " ", "spectrum"}], " ", "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"Pk", "[", "k_", "]"}], ":=", 
     RowBox[{"100", "/", 
      RowBox[{"(", 
       RowBox[{"1", "+", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"20", "k"}], ")"}], "^", "2"}], "+", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"5", "k"}], ")"}], "^", "4"}], "+", 
        RowBox[{
         RowBox[{"(", "k", ")"}], "^", "6"}]}], ")"}]}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"kmin", "=", "1*^-5"}], ";", "\[IndentingNewLine]", 
    RowBox[{"kmax", "=", "250"}], ";"}], ",", "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"Load", " ", "power", " ", "spectrum", " ", "from", " ", "file"}],
     " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"{", 
      RowBox[{"kvals", ",", "Pkvals"}], "}"}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{"Import", "[", 
        RowBox[{
        "\"\<../../models/\>\"", "<>", "modelname", "<>", 
         "\"\</powerspectrum.csv\>\""}], "]"}], "[", 
       RowBox[{"[", 
        RowBox[{"2", ";;"}], "]"}], "]"}], "//", "Transpose"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"kmin", "=", 
     RowBox[{"kvals", "[", 
      RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"kmax", "=", 
     RowBox[{"kvals", "[", 
      RowBox[{"[", 
       RowBox[{"-", "1"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"Pk", "=", 
     RowBox[{"Interpolation", "[", 
      RowBox[{
       RowBox[{"Transpose", "[", 
        RowBox[{"{", 
         RowBox[{"kvals", ",", "Pkvals"}], "}"}], "]"}], ",", 
       RowBox[{"Method", "->", "\"\<Spline\>\""}]}], "]"}]}], ";"}]}], 
