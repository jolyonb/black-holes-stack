Cell["Load the power spectrum.", "Text",ExpressionUUID->"aab7685f-66c1-48c2-a0cf-ee6c84591023"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"params", " ", "=", " ", 
   RowBox[{"Import", "[", 
    RowBox[{
    "\"\<../../models/\>\"", "<>", "modelname", "<>", 
     "\"\</powerspectrum-params.txt\>\""}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"testps", "=", 
   RowBox[{"StringContainsQ", "[", 
    RowBox[{"params", ",", "\"\<test_ps: True\>\""}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{"If", "[", 
  RowBox[{"testps", ",", "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"Test", " ", "power", " ", "spectrum"}], " ", "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"Pk", "[", "k_", "]"}], ":=", 
     RowBox[{"Exp", "[", 
      RowBox[{"-", "k"}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"kmin", "=", "0"}], ";", "\[IndentingNewLine]", 
    RowBox[{"kmax", "=", "20"}], ";"}], ",", "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"Load", " ", "power", " ", "spectrum", " ", "from", " ", "file"}],
     " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"{", 
      RowBox[{"kvals", ",", "Pkvals"}], "}"}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{"Import", "[", 
        RowBox[{
        "\"\<../../models/\>\"", "<>", "modelname", "<>", 
         "\"\</powerspectrum.csv\>\""}], "]"}], "[", 
       RowBox[{"[", 
        RowBox[{"2", ";;"}], "]"}], "]"}], "//", "Transpose"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"kmin", "=", 
     RowBox[{"kvals", "[", 
      RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"kmax", "=", 
     RowBox[{"kvals", "[", 
      RowBox[{"[", 
       RowBox[{"-", "1"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"Pk", "=", 
     RowBox[{"Interpolation", "[", 
      RowBox[{"Transpose", "[", 
       RowBox[{"{", 
        RowBox[{"kvals", ",", "Pkvals"}], "}"}], "]"}], "]"}]}], ";"}]}], 
  "\[IndentingNewLine]", "]"}]}], "Input",
 CellLabel->"In[43]:=",ExpressionUUID->"c9c49599-2870-4609-a2a4-0862920927bb"],

