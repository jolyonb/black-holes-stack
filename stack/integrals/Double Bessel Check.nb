Cell["Compare Integrals", "Section",ExpressionUUID->"9d977029-1fac-4033-a1ef-90d1088695ec"],

Cell["Compare python E integrals to Mathematica integrals.", "Text",ExpressionUUID->"88b71fd5-ff14-49d5-be2b-46d489f3ab51"],

Cell[BoxData[
 RowBox[{
  RowBox[{"doEintegral", "[", 
   RowBox[{"ell_", ",", "r_"}], "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"res1", ",", "res2", ",", "res3", ",", "res4", ",", "res5"}], 
     "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"res1", "=", 
      RowBox[{"NIntegrate", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"k", "^", "2"}], " ", 
         RowBox[{"Pk", "[", "k", "]"}], 
         RowBox[{
          RowBox[{"SphericalBesselJ", "[", 
           RowBox[{"ell", ",", 
            RowBox[{"k", " ", "r"}]}], "]"}], "^", "2"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"k", ",", "kmin", ",", "10"}], "}"}], ",", 
        RowBox[{"WorkingPrecision", "\[Rule]", "25"}], ",", 
        RowBox[{"AccuracyGoal", "\[Rule]", "15"}], ",", 
        RowBox[{"PrecisionGoal", "\[Rule]", "15"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"res2", "=", 
      RowBox[{"NIntegrate", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"k", "^", "2"}], " ", 
         RowBox[{"Pk", "[", "k", "]"}], 
         RowBox[{
          RowBox[{"SphericalBesselJ", "[", 
           RowBox[{"ell", ",", 
            RowBox[{"k", " ", "r"}]}], "]"}], "^", "2"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"k", ",", "10", ",", "50"}], "}"}], ",", 
        RowBox[{"WorkingPrecision", "\[Rule]", "25"}], ",", 
        RowBox[{"AccuracyGoal", "\[Rule]", "15"}], ",", 
        RowBox[{"PrecisionGoal", "\[Rule]", "15"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"res3", "=", 
      RowBox[{"NIntegrate", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"k", "^", "2"}], " ", 
         RowBox[{"Pk", "[", "k", "]"}], 
         RowBox[{
          RowBox[{"SphericalBesselJ", "[", 
           RowBox[{"ell", ",", 
            RowBox[{"k", " ", "r"}]}], "]"}], "^", "2"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"k", ",", "50", ",", "100"}], "}"}], ",", 
        RowBox[{"WorkingPrecision", "\[Rule]", "25"}], ",", 
        RowBox[{"AccuracyGoal", "\[Rule]", "15"}], ",", 
        RowBox[{"PrecisionGoal", "\[Rule]", "15"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"res4", "=", 
      RowBox[{"NIntegrate", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"k", "^", "2"}], " ", 
         RowBox[{"Pk", "[", "k", "]"}], 
         RowBox[{
          RowBox[{"SphericalBesselJ", "[", 
           RowBox[{"ell", ",", 
            RowBox[{"k", " ", "r"}]}], "]"}], "^", "2"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"k", ",", "100", ",", "150"}], "}"}], ",", 
        RowBox[{"WorkingPrecision", "\[Rule]", "25"}], ",", 
        RowBox[{"AccuracyGoal", "\[Rule]", "15"}], ",", 
        RowBox[{"PrecisionGoal", "\[Rule]", "15"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"res5", "=", 
      RowBox[{"NIntegrate", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"k", "^", "2"}], " ", 
         RowBox[{"Pk", "[", "k", "]"}], 
         RowBox[{
          RowBox[{"SphericalBesselJ", "[", 
           RowBox[{"ell", ",", 
            RowBox[{"k", " ", "r"}]}], "]"}], "^", "2"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"k", ",", "150", ",", "kmax"}], "}"}], ",", 
        RowBox[{"WorkingPrecision", "\[Rule]", "25"}], ",", 
        RowBox[{"AccuracyGoal", "\[Rule]", "15"}], ",", 
        RowBox[{"PrecisionGoal", "\[Rule]", "15"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"4", "\[Pi]", " ", 
      RowBox[{"(", 
       RowBox[{"res1", "+", "res2", "+", "res3", "+", "res4", "+", "res5"}], 
       ")"}]}]}]}], "\[IndentingNewLine]", "]"}]}]], "Input",
 CellLabel->
  "In[359]:=",ExpressionUUID->"f97e27fc-f28d-4471-8f6f-b13e3e6a99fa"],

